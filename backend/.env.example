# Environment Variables for AI Governance Backend
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://aigovernance:password123@localhost:5432/aigovernance_db

# API Security
API_KEY_SECRET=dev-secret-key-change-in-production

# CORS Configuration  
ALLOWED_ORIGINS=http://localhost:3000

# Supermemory API
SUPERMEMORY_API_URL=https://api.supermemory.ai
SUPERMEMORY_API_KEY=your_supermemory_api_key_here

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-lite

